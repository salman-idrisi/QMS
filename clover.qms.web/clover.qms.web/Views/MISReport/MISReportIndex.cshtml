<script type="text/javascript">
    $(document).ready(function () {
        $('#dateStart').dateTimePicker({ showTime: true, dateFormat: 'dd-M-yy HH:mm' });
        $('#dateEnd').dateTimePicker({ showTime: true, dateFormat: 'dd-M-yy HH:mm' });

    })
    function ValidateEndDate() {
        var startDate = $("#dateStart").val();
        var endDate = $("#dateEnd").val();
        //if (startDate == '') {
        //    alert("Enter start date");
        //}
        //if (endDate == '') {
        //    alert("Enter end date");
        //}
        if (startDate != '' && endDate != '') {
            if (Date.parse(startDate) > Date.parse(endDate)) {
                $('#validationDate').html('<font color="Red">End date should be greater than start date</font>');
                $("#dateEnd").val('');
                // alert("Start date should not be greater than end date");

            }
        }
        return false;
    };

</script>
