                @*<input type="submit" value="NC Aging" id="btnvalidate" formaction="@Url.Action("DisplayNCAgingReport","MISReport")" class="btn btn-primary form-control" />*@
                <input type="submit" value="NC Aging" onclick="ValidateEndDate()" class="btn btn-primary form-control" />
            </div>
        </div>

    }
