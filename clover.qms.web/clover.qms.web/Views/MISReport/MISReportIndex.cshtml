<body style="margin-top:70PX;height:80PX" ;>
    @using (Html.BeginForm("MISReportIndex", "MISReport", FormMethod.Post))
    {
        <div>
            <div class="row">
                <div class="col-sm-12 " @*btn btn-success*@>
                    <h3 style="text-align:center;color:darkred">NC Aging Report</h3>
                </div>
            </div>
        </div><br />
        <div class="row">
            <div class="col-sm-2 text-lg-left"> <p style="color:darkblue;font-size:18px">From Date:</p></div>
            <div class="col-sm-3" id="dateStart" data-target-input="nearest">


                <input class="form-control datetimepicker-input" name="startdate" id="dateStart" autocomplete="off" placeholder="Start Date" />

                <span class="input-group-append" data-target="#dateStart" data-toggle="datetimepicker">
                    <span class="input-group-text"><i class="fa fa-calendar"></i></span>
                </span>

            </div>
            <div class="col-sm-2 text-lg-left"> <p style="color:darkblue;font-size:18px">To Date:</p></div>
            <div class="col-sm-3" id="dateEnd" data-target-input="nearest">


                <input class="form-control datetimepicker-input" name="enddate" id="dateEnd" autocomplete="off" placeholder="End Date" />


                <span class="input-group-append" data-target="#dateEnd" data-toggle="datetimepicker">
                    <span class="input-group-text"><i class="fa fa-calendar"></i></span>
                </span>

            </div>

            @*<div class="col-sm-2"><p style="color:darkblue;font-size:18px">To Date:</p></div>
